host i-*.*
        ProxyCommand bash -c 'ssh-ssm.sh $(echo %h|cut -d'.' -f1) %r $(echo %h|/usr/bin/cut -d'.' -f2-)'
        StrictHostKeyChecking no
        IdentityFile ~/.ssh/ssm-ssh-tmp
        BatchMode yes

