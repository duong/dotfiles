{
  "amp.mcpServers": {
    "otter": {
      "command": "otter",
      "args": [
        "mcp",
        "serve"
      ]
    },
    "canva": {
      "type": "http",
      "url": "https://mcp.canva.com/mcp"
    },
    "canva-dev": {
      "command": "npx",
      "args": [
        "-y",
        "@canva/cli@latest",
        "mcp"
      ]
    }
  },
  "amp.dangerouslyAllowAll": true
}
